<div class="entry">
    <table class="table table-striped table-hover">
        <thead>
            <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Price</th>
            <th scope="col">Imagen</th>
            </tr>
        </thead>
        <tbody id="trTable">
            <% listProducts.forEach(product => { %>
            <tr>
                <td> <%=product.id %> </td>
                <td> <%=product.title %> </td>
                <td> <%=product.price %> </td>
                <td><img src="<%=product.url %>" alt="<%=product.title %>" width="25"></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>
